/**reset css**/
dl,ul,ol,menu,li {list-style:none}
a, a:active, a:hover {text-decoration:none}
/**reset css end**/

/**banner set**/

/**banner set end**/
.banner{
	padding-left:25%;
}
/**recipe_list set**/
.recipe_list{
	border:1px solid #789262;
	margin-top:30px;
	max-height:600px;
	overflow-x: hidden; 
	overflow-y: auto; 
	padding:0;
}
.recipe_list ul{
	width:100%;
	text-align:center;
	margin-left:-15px;
}
.recipe_list li{
	border-bottom:1px dashed red;
}
/**recipe_details**/
.recipe_detail{
	margin-top:30px;
	margin-left:8.3%;
	border:1px solid #789262;
}
/**recipe_details end**/
